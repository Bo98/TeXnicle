	Journal = {Classical and Quantum Gravity},
	Number = {5},
	Pages = {S573},
	Title = {Measuring random force noise for LISA aboard the LISA pathfinder mission},
	Volume = {21},
	Year = {2004},
	Bdsk-Url-1 = {http://stacks.iop.org/0264-9381/21/i=5/a=028}}

